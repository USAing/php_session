{
    "name": "yakeing/php_session",
    "description": "This is a SESSION class function package, use more easily, just a few simple functions can use it.",
    "keywords": ["session", "php-session"],
    "homepage": "https://github.com/yakeing/php_session",
    "type": "symfony-bundle",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "yakeing",
            "homepage": "http://weibo.com/yakeing"
        }
    ],
    "autoload": {
        "psr-4": {
            "php_session\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-session": "*"
    },
    "require-dev": {
         "phpunit/phpunit": "5.7"
    },
    "scripts": {
        "report-coverage": "codecov"
    },
    "extra":{
        "branch-alias":{
            "dev-master": "1.x-dev"
        }
    }
}
